def show_loading():
    for _ in range(3):
        print("loading...")
a=5
b=6
print(a+b)
show_loading()
print(a-b)
show_loading()
print(a*b)
show_loading()

def sheldon_knock(name,times=3):
    for i in range(times):
        print("knock knock knock",name)
sheldon_knock("larry",5)

